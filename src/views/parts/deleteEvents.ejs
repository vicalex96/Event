<div class="row">
      <!-- ITEMS -->
      <% for(var i = 0; i < evento.length; i++) { %>
        <div class="col-lg-4 col-sm-4 portfolio-item">
          <div class="card h-200">
            <a href="#"><img class="card-img-top" src="<%= evento[i].image %>" alt=""></a>
            <div class="card-body">
              <h4 class="card-title"><%= evento[i].title %></h4>
              <p class="card-text">
                <%= evento[i].description %>
              </p>
          </div>
          <div class="slide-footer">
              <form class= "link" action="/editor/<%= evento[i]._id %>?_method=DELETE"
                method= "POST">
                <span class="pull-right buttons">
                  <button class="btn btn-sm btn-default"><i class="fa fa-fw fa-eye"></i> eliminar</button>
                </span>
              </form>        
          </div>



        </div>
      </div>
     <% } %>
</div>
